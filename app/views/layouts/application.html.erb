<!DOCTYPE html>
<html>
<head>
  <title>AdministraAppBeta</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

	<%=link_to 'Residentes', residentes_path%> |
	<%=link_to 'Apartamentos', apartamentos_path%> |
	<%=link_to 'Vehiculos', vehiculos_path%> |
	<%=link_to 'Usuarios', usuarios_path%>
	<% if @current_user %>
	Hola <%= @current_user.nombre%> |
	<%= link_to 'Salir', sesion_path(0), method: :delete %>
	<% else %>
		<%= link_to 'Ingresar', new_sesion_path %>
	<% end %>

	<%= yield %> <!--Mete las demas vistas-->


</body>
</html>
